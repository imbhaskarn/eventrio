{
  "name": "eventrio_api",
  "version": "1.0.0",
  "description": "eventrio api",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:all": "sequelize-cli db:migrate:all",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check './**/*.{js,ts,json}' --config ./.prettierrc",
    "prettier:fix": "prettier --write './**/*.{js,ts,json}' --config ./.prettierrc",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "author": "eventrio team",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1190.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "twilio": "^3.81.0"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  }
}
